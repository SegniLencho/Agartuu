{
    "database": {
        "rules": "database"
    },
    "hosting": {
        "site": "agartuuoromo",
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }]
    },
    "emulators": {
